{
  "packages": ["org.gobiiproject.gobiiprocess.digester.machine", "org.gobiiproject.gobiiprocess.commons.machine"],
  "prototypes": {
    "log": {
      "sideEffects": ["log/info", "log/warn", "log/error"]},
    "jobStatusLog": {
      "sideEffects": [
        {"name": "log/state", "config": {"value": ["jobStatus"]}}
      ]}
  },
  "steps": {
    "digester/setup": {
      "transition": "digest/setup",
      "validation": "digest/setup"
    },
    "digester/instructionFileRead": {
      "transition": "digest/instructionFileRead",
      "validation": "digest/instructionFileRead"
    }
  },
  "pipelines": {
    "pipeline/digester/prod": {
      "prototypes": ["log"],
      "steps": ["pipeline/digester/instruction-loader", "pipeline/digester/core"]
    },
    "pipeline/digester/instruction-loader": {
      "prototypes": ["log"],
      "steps": ["digest"]
    },
    "pipeline/digester/core": {
      "prototypes": ["log"],
      "steps": ["digester/instructionFileRead"]
    }
  }
}